{
  "name": "snappshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn run watch:css\" \"next dev\"",
    "build": "yarn run build:css && next build",
    "start": "next start",
    "lint": "next lint",
    "watch:css": "postcss ./public/style.css -o ./src/assets/main.css --watch",
    "build:css": "postcss ./public/style.css -o ./src/assets/main.css --minify"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "concurrently": "^7.4.0",
    "cssnano": "^5.1.13",
    "json-server": "^0.17.1",
    "next": "^13.0.6",
    "postcss-cli": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.18",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.4"
  }
}
